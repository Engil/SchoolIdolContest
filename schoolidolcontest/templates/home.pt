<html>
  <head>
    <meta http-equiv="content-type" content="text/html; charset=utf-8"0>

    <title>Home</title>

  </head>
  <body>
      <form action="/__vote" method="post">
        <input type="submit" name="left" value="${left.name.split(' ')[-1]} is best"/>
        <input type="submit" name="right" value="${right.name.split(' ')[-1]} is best"/>
        </form>
    <div id="name">
      <p>${left.id}</p>
      <p>${left.name}</p>
      <img src="${left.card_url if left.card_url and idolized_left else left.card_idolized_url}" width=100></img>
      <p>${right.id}</p>
      <p>${right.name}</p>
      <img src="${right.card_url if right.card_url and idolized_right else right.card_idolized_url}" width=100></img>
    </div>
    <div>
      </div>
    </body>
  </html>
